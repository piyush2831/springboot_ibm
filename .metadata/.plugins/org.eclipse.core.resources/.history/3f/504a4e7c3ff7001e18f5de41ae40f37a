package learning1.java;

import java.util.*;

public class Day1 {
	public static void main(String[] args) {
		System.out.println("input taking");
		Scanner sc= new Scanner(System.in);
		System.out.println("enter no 1");
		int a = sc.nextInt();
		System.out.println("enter no 2");
		int b = sc.nextInt();
		int sum = a+b;
		int sub= a-b;
		System.out.println("The sum of 2 number are:  "+ (sum)+" " +(sub));
		//System.out.println(sum);
		
		
		
		
		
	}

}
